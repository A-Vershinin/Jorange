{
    "remove-empty-rulesets": true,
    "always-semicolon": true,
    "color-case": "lower",
    "block-indent": "2",
    "color-shorthand": false,
    "element-case": "lower",
    "leading-zero": false,
    "quotes": "double",
    "sort-order-fallback": "abc", "sort-order":
      [
        "position",
        "display",
        "float",
        "width",
        "height",
        "margin",
        "padding",
        "fontSize",
        "fontFamily",
        "color",
        "background",
        "border",
        "opacity",
        "transform"
        "transition"
      ],
    "space-before-colon": " ",
    "space-after-colon": " ",
    "space-before-combinator": "1",
    "space-after-combinator": " ",
    "space-between-declarations": "\n",
    "space-before-opening-brace": "1",
    "space-after-opening-brace": "1",
    "space-after-selector-delimiter": "1",
    "space-before-selector-delimiter": "0",
    "space-before-closing-brace": "\n",
    "strip-spaces": true,
    "tab-size": 2,
    "vendor-prefix-align": false,
    "lines-between-rulesets": 0
}
